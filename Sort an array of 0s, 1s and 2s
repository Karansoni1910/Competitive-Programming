#include<bits/stdc++.h>
using namespace std;
class Solution
{
    public:
    void sort012(int a[], int n)
    {
        int lo=0,hi=n-1,mid=0;
        while(mid<=hi){
            switch(a[mid]){
                case 0: swap(a[lo],a[mid]);
                        lo++;
                        mid++;
                        break;
                case 1: mid++;
                        break;
                case 2: swap(a[mid],a[hi]);
                        hi--;
                        break;
            }
        }
    }
    
};
int main() {

    int t;
    cin >> t;

    while(t--){
        int n;
        cin >>n;
        int a[n];
        for(int i=0;i<n;i++){
            cin >> a[i];
        }

        Solution ob;
        ob.sort012(a, n);

        for(int i=0;i<n;i++){
            cout << a[i]  << " ";
        }

        cout << endl;
        
        
    }
    return 0;
}
