// { Driver Code Starts
#include <bits/stdc++.h>
using namespace std;

 // } Driver Code Ends
class Solution {
  public:
    // Function to detect cycle in an undirected graph.
    vector<bool> v;
    
    bool dfs(vector<int> adj[],int i,int p){
        // cout<<i<<" : "<<p<<" / ";
        if(v[i]) return true;
        v[i]=true;
        for(int j=0;j<adj[i].size();j++){
            if(adj[i][j]!=p && dfs(adj,adj[i][j],i)) return true;
        }
        return false;
        
        
    }
    bool isCycle(int V, vector<int> adj[]) {
        // Code here
        v.resize(V,false);
        for(int i=0;i<V;i++){
            // cout<<i<<" < ";
            if(!v[i] && dfs(adj,i,-1)) return true;
            // cout<<endl;
        }
        return false;
    }
};

// { Driver Code Starts.
int main() {
    int tc;
    cin >> tc;
    while (tc--) {
        int V, E;
        cin >> V >> E;
        vector<int> adj[V];
        for (int i = 0; i < E; i++) {
            int u, v;
            cin >> u >> v;
            adj[u].push_back(v);
            adj[v].push_back(u);
        }
        Solution obj;
        bool ans = obj.isCycle(V, adj);
        if (ans)
            cout << "1\n";
        else
            cout << "0\n";
    }
    return 0;
}  // } Driver Code Ends
